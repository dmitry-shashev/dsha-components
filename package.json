{
  "name": "dsha-components",
  "repository": {
    "type": "git",
    "url": "https://github.com/dmitry-shashev/dsha-components"
  },
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": true,
  "version": "0.0.3",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./style.css": {
      "import": "./dist/style.css"
    }
  },
  "scripts": {
    "prepare": "husky install",
    "prettier-format": "prettier --write . && git add --all",
    "build": "tsc && vite build",
    "dev": "storybook dev -p 6006",
    "test": "vitest run",
    "test-dev": "vitest",
    "test-ui": "vitest --ui",
    "lint": "eslint src"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18.17.1",
    "pnpm": ">=8.7.4"
  },
  "prettier": {
    "arrowParens": "always",
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "bracketSpacing": true
  },
  "author": {
    "name": "Dmitry",
    "email": "dy.shashev@gmail.com",
    "url": "https://github.com/dmitry-shashev"
  },
  "license": "MIT",
