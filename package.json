    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-storybook": "0.6.13",
    "jsdom": "22.1.0",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "1.66.1",
    "storybook": "^7.4.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "3.5.3",
    "vite-plugin-linter": "2.0.5",
    "vitest": "0.34.4"
